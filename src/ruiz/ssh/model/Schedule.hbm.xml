<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="ruiz.ssh.model">

    <!-- class 和 table 对应 -->
    <class name="Schedule" table="schedule">
        <!-- 一一对应 -->
        <id name="id" column="id"><generator class="increment" /></id>
        <property name="name" column="name"></property>        
        <property name="info" column="info"></property>
        <property name="note" column="note"></property>
        <property name="userid" column="userid"></property>  
        <property name="starttime" column="starttime"></property>
        <property name="endtime" column="endtime"></property>  
    </class>
</hibernate-mapping>