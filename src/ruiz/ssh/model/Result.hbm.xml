<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="ruiz.ssh.model">

    <!-- class 和 table 对应 -->
    <class name="Result" table="result">
        <!-- 一一对应 -->
        <id name="id" column="id"><generator class="increment" /></id>
        <property name="schid" column="schid"></property>        
        <property name="shipid" column="shipid"></property>
        <property name="order" column="order"></property>   
        <property name="baseid" column="baseid"></property> 
        <property name="loadlistid" column="loadlistid"></property> 
    </class>
</hibernate-mapping>